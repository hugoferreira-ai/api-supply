{
  "kind": "collectionType",
  "collectionName": "subcategorias",
  "info": {
    "singularName": "subcategoria",
    "pluralName": "subcategorias",
    "displayName": "Subcategoria"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nome": {
      "type": "string",
      "required": true
    },
    "codigo": {
      "type": "string",
      "required": true
    },
    "categoria": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::categoria.categoria"
    },
    "descricao": {
      "type": "string"
    },
    "padrao": {
      "type": "boolean"
    },
    "produtos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::produto.produto",
      "mappedBy": "subcategoria"
    }
  }
}
